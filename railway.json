{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile.backend"
  },
  "deploy": {
    "startCommand": "gunicorn -w 2 -k uvicorn.workers.UvicornWorker --timeout 400 app.main:app",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100
  }
}
